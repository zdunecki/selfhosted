{
  "applicationId": "io.livesession.selfhosted",
  "version": "0.1.0",
  "defaultMode": "window",
  "documentRoot": "/vite-src/dist/",
  "url": "/",
  "port": 3000,
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "window.*",
    "os.*"
  ],
  "singlePageServe": true,
  "modes": {
    "window": {
      "title": "SelfHosted Installer",
      "width": 1000,
      "height": 900,
      "minWidth": 1000,
      "maxWidth": 1000,
      "minHeight": 640,
      "center": true,
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "selfhosted-desktop",
    "resourcesPath": "/vite-src/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "6.2.0",
    "clientVersion": "6.2.0",
    "frontendLibrary": {
      "patchFile": "/vite-src/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/vite-src/",
      "initCommand": "npm install",
      "devCommand": "npm run dev",
      "buildCommand": "npm run build"
    }
  }
}

